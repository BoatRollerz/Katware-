	ESP.Name = "ESP"
	ESP.Parent = Frame
	ESP.BackgroundColor3 = Color3.fromRGB(56, 56, 56)
	ESP.Position = UDim2.new(0.045748964, 0, 0.193718433, 0)
	ESP.Size = UDim2.new(0, 150, 0, 37)
	ESP.Font = Enum.Font.SourceSans
	ESP.Text = "ESP"
	ESP.TextColor3 = Color3.fromRGB(255, 15, 231)
	ESP.TextScaled = true
	ESP.TextSize = 14.000
